<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_graph"
    app:startDestination="@id/nav_home">

    <fragment
        android:id="@+id/nav_login"
        android:name="com.example.vrades.ui.fragments.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login">
        <action
            android:id="@+id/action_nav_login_to_nav_home"
            app:destination="@id/nav_home" />
        <action
            android:id="@+id/action_nav_login_to_nav_register"
            app:destination="@id/nav_register" />
        <action
            android:id="@+id/action_nav_login_to_nav_forgot"
            app:destination="@id/nav_forgot" />
    </fragment>
    <fragment
        android:id="@+id/nav_register"
        android:name="com.example.vrades.ui.fragments.RegisterFragment"
        android:label="fragment_register"
        tools:layout="@layout/fragment_register">

        <action
            android:id="@+id/action_nav_register_to_nav_login"
            app:destination="@id/nav_login" />
        <action
            android:id="@+id/action_nav_register_to_nav_tutorial"
            app:destination="@id/nav_tutorial" />
    </fragment>
    <fragment
        android:id="@+id/nav_home"
        android:name="com.example.vrades.ui.fragments.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home">
        <action
            android:id="@+id/action_nav_home_to_nav_profile"
            app:destination="@id/nav_profile" />
        <action
            android:id="@+id/action_nav_home_to_nav_settings"
            app:destination="@id/nav_settings" />
        <action
            android:id="@+id/action_nav_home_to_nav_about"
            app:destination="@id/nav_about" />
        <action
            android:id="@+id/action_nav_home_to_nav_test"
            app:destination="@+id/nav_test" />
        <action
            android:id="@+id/action_nav_home_to_mobile_navigation"
            app:destination="@+id/mobile_navigation" />
        <action
            android:id="@+id/action_nav_home_to_nav_login"
            app:destination="@id/nav_login" />
        <action
            android:id="@+id/action_nav_home_to_nav_tutorial"
            app:destination="@id/nav_tutorial" />
    </fragment>

    <fragment
        android:id="@+id/nav_about"
        android:name="com.example.vrades.ui.fragments.AboutFragment"
        android:label="AboutFragment" >
        <action
            android:id="@+id/action_nav_about_to_nav_home"
            app:destination="@id/nav_home" />
    </fragment>
    <fragment
        android:id="@+id/nav_settings"
        android:name="com.example.vrades.ui.fragments.SettingsFragment"
        android:label="SettingsFragment" >
        <action
            android:id="@+id/action_nav_settings_to_nav_home"
            app:destination="@id/nav_home" />
    </fragment>
    <fragment
        android:id="@+id/nav_solutions"
        android:name="com.example.vrades.ui.fragments.SolutionsFragment"
        android:label="SolutionsFragment" >
        <action
            android:id="@+id/action_nav_solutions_to_nav_profile"
            app:destination="@id/nav_profile" />
    </fragment>
    <fragment
        android:id="@+id/nav_details"
        android:name="com.example.vrades.ui.fragments.DetailsFragment"
        android:label="DetailsFragment" >
        <action
            android:id="@+id/action_nav_details_to_nav_results"
            app:destination="@id/nav_results" />
        <action
            android:id="@+id/action_nav_details_to_nav_home"
            app:destination="@id/nav_home" />
    </fragment>
    <fragment
        android:id="@+id/nav_profile"
        android:name="com.example.vrades.ui.fragments.MyProfileFragment"
        android:label="MyProfileFragment" >
        <action
            android:id="@+id/action_nav_profile_to_nav_solutions"
            app:destination="@id/nav_solutions" />
        <action
            android:id="@+id/action_nav_profile_to_nav_results"
            app:destination="@id/nav_results" />
        <action
            android:id="@+id/action_nav_profile_to_nav_home"
            app:destination="@id/nav_home" />
    </fragment>
    <fragment
        android:id="@+id/nav_results"
        android:name="com.example.vrades.ui.fragments.ResultsFragment"
        android:label="ResultsFragment" >
        <action
            android:id="@+id/action_nav_results_to_nav_profile"
            app:destination="@id/nav_profile" />
        <action
            android:id="@+id/action_nav_results_to_nav_details"
            app:destination="@id/nav_details" />
    </fragment>
    <fragment
        android:id="@+id/nav_test"
        android:name="com.example.vrades.ui.fragments.TestFragment"
        android:label="TestFragment" >
        <action
            android:id="@+id/action_nav_test_to_nav_home"
            app:destination="@id/nav_home" />
        <action
            android:id="@+id/action_nav_test_to_nav_details"
            app:destination="@id/nav_details" />
    </fragment>
    <fragment
        android:id="@+id/nav_forgot"
        android:name="com.example.vrades.ui.fragments.ForgotPasswordFragment"
        android:label="ForgotPasswordFragment">
        <action
            android:id="@+id/action_nav_forgot_to_nav_login"
            app:destination="@id/nav_login" />
    </fragment>
    <activity
        android:id="@+id/splashScreenActivity"
        android:name="com.example.vrades.ui.activities.StartActivity"
        android:label="SplashScreenActivity" />
    <fragment
        android:id="@+id/nav_tutorial"
        android:name="com.example.vrades.ui.fragments.tutorial.TutorialParentFragment"
        android:label="TutorialParentFragment" >
        <action
            android:id="@+id/action_nav_tutorial_to_nav_home"
            app:destination="@id/nav_home" />
    </fragment>

</navigation>